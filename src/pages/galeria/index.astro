---
import GaleriaLayout from "../../layouts/GaleriaLayout.astro";
import { getCollection } from "astro:content";


// Replace the string file paths with the imported images


const posts = (await getCollection("galeria")) as Array<{ data: { slug: string; title: string; images: any[] } }>;
const images = posts.map(post => post.data.images).flat();

---

<GaleriaLayout images={images}/>

